{
  "name": "shopbebin-v2",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node tools/build.js && npx tailwindcss -i ./src/views/input.css -o ./public/dist/output.css",
    "vercel-build": "pnpm schema && node tools/build.js && npx tailwindcss -i ./src/views/input.css -o ./public/dist/output.css",
    "start": "node ./public/dist/api.js",
    "schema": "npx prisma generate",
    "dev": "concurrently \"node tools/serve.js\" \"nodemon public/dist/api.js\" \"npx tailwindcss --jit -i ./src/views/input.css -o ./public/dist/output.css --watch\"",
    "dev:brave": "pnpm dev \"nodemon --watch src/views --exec 'osascript tools/brave.scpt' -e js,ts,jsx,tsx\"",
    "dev:chrome": "pnpm dev \"nodemon --watch src/views --exec 'osascript tools/chrome.scpt' -e js,ts,jsx,tsx\"",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx && echo \"\u001b[32mLinting passed!\u001b[0m\" || echo \"\u001b[31mLinting failed!\u001b[0m\"",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix && echo \"\u001b[32mLinting passed!\u001b[0m\" || echo \"\u001b[31mLinting failed!\u001b[0m\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/chart.js": "^2.9.41",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.1",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-valtio": "^0.6.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
